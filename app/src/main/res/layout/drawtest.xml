<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:visibility="visible">
    <RelativeLayout
        android:id="@+id/drawer"
        android:layout_width="300dp"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:background="#ffffff"
        android:layout_gravity="start">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="300dp"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/drawerlinear"
                    android:layout_width="300dp"
                    android:layout_height="100dp"
                    android:background="#eeeeee"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="180dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_marginLeft="20dp"
                        android:gravity="center_vertical"
                        >

                        <TextView
                            android:id="@+id/main_id"
                            android:layout_marginTop="5dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="eqweqwe"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/main_email"
                            android:textSize="18sp"
                            android:text="qqqqqq@asdasd"
                            android:textColor="#000000"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"></TextView>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="30dp"
                            android:layout_marginTop="2dp"
                            android:background="@drawable/ic_heart_off"></TextView>
                        <Button
                            android:id="@+id/my_apt"
                            android:layout_width="220dp"
                            android:layout_height="70dp"
                            android:textSize="20sp"
                            android:fontFamily="@font/spoqabold"
                            android:background="@drawable/non_edittext"
                            android:layout_centerHorizontal="true"
                            android:stateListAnimator="@null"
                            android:text="    관심 아파트"
                            android:gravity="center_vertical"/>
                    </LinearLayout>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@drawable/line_drawer"></TextView>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="30dp"
                            android:background="@drawable/rank"></TextView>
                        <Button
                            android:id="@+id/apt_rank"
                            android:layout_width="220dp"
                            android:layout_height="70dp"
                            android:textSize="20sp"
                            android:fontFamily="@font/spoqabold"
                            android:background="@drawable/non_edittext"
                            android:layout_centerHorizontal="true"
                            android:stateListAnimator="@null"
                            android:text="    아파트 랭킹"
                            android:gravity="center_vertical"/>
                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@drawable/line_drawer"></TextView>

                    <TextView
                        android:layout_marginTop="420dp"
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@drawable/line_drawer"></TextView>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginTop="16dp"
                        android:layout_marginRight="18dp">
                        <Button
                            android:id="@+id/my_page"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:stateListAnimator="@null"
                            android:background="@drawable/mypage_gray"></Button>
                        <Button
                            android:layout_marginLeft="10dp"
                            android:id="@+id/logout"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:stateListAnimator="@null"
                            android:background="@drawable/logout_gray"></Button></LinearLayout>
                </LinearLayout>

            </LinearLayout>
            <ImageButton
                android:id="@+id/btn_CloseDrawer"
                android:layout_width="110dp"
                android:layout_height="match_parent"
                android:background="@drawable/non_edittext">
            </ImageButton>

        </LinearLayout>


    </RelativeLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>